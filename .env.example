APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Database Configuration (Docker PostgreSQL 17)
# Use docker-compose up -d to start the database
DB_CONNECTION=pgsql
DB_HOST=127.0.0.1
DB_PORT=5432
DB_DATABASE=kibaops
DB_USERNAME=kibaops_user
DB_PASSWORD=kibaops_password

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

# Redis Configuration (Docker Redis 7)
# Use docker-compose up -d to start Redis
REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# GitHub App Configuration
# Create a GitHub App at https://github.com/settings/apps
KIBAOPS_GITHUB_APP_ID=
KIBAOPS_GITHUB_APP_NAME=
KIBAOPS_GITHUB_APP_SECRET=
KIBAOPS_GITHUB_APP_PRIVATE_KEY=
KIBAOPS_GITHUB_APP_WEBHOOKS_SECRET=
KIBAOPS_GITHUB_CALLBACK_URL=

# Development URL for webhooks (use ngrok for local development)
NGROK_URL=

# HashiCorp Vault Configuration (Docker)
# Use docker-compose up -d to start Vault
VAULT_ADDR=http://127.0.0.1:8200
VAULT_TOKEN=myroot
VAULT_READ_ROLE_ID=kibaops-reads
VAULT_READ_SECRET_ID=7c4a2e8f-9b1d-4a3c-8e5f-2d7b9c1a4e6f
VAULT_WRITE_ROLE_ID=kibaops-writes
VAULT_WRITE_SECRET_ID=3f8e1c9a-5d2b-4e7f-9a1c-6b8d4f2e7a9c
